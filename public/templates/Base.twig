<html lang="en" class="light-style layout-menu-fixed" dir="ltr">
	<head>
		<meta charset="utf-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0"/>

		<title>{{ pageTitle }}
			|
			{{ config.CMSControl_SiteName|default('CrispCMS') }}</title>

		<meta
		name="description" content=""/>

		<!-- Favicon -->
		<link
		rel="icon" type="image/x-icon" href="{{ includeResource("img/favicon/favicon.ico") }}"/>

		<!-- Fonts -->
		<link rel="preconnect" href="https://fonts.googleapis.com"/>
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin/>
		<link
		href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet"/>

		<!-- Icons. Uncomment required icon fonts -->
		<link
		rel="stylesheet" href="{{ includeResource("vendor/fonts/boxicons.css") }}"/>

		<!-- Core CSS -->
		<link rel="stylesheet" href="{{ includeResource("vendor/css/core.css") }}" class="template-customizer-core-css"/>
		<link rel="stylesheet" href="{{ includeResource("vendor/css/theme-default.css") }}" class="template-customizer-theme-css"/>
		<link
		rel="stylesheet" href="{{ includeResource("css/demo.css") }}"/>

		<!-- Vendors CSS -->
		<link rel="stylesheet" href="{{ includeResource("vendor/libs/perfect-scrollbar/perfect-scrollbar.css") }}"/>

		<link rel="stylesheet" href="{{ includeResource("vendor/libs/apex-charts/apex-charts.css") }}"/>
		<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.min.css"/>

		{% if load_quill ?? false %}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/elfinder/2.1.65/css/elfinder.min.css"/>
			<link rel="stylesheet" href="{{ includeResource("vendor/libs/ckeditor5/ckeditor5.css") }}"/>
		{% endif %}
		<link
		rel="stylesheet" href="{{ includeResource("vendor/fontawesome/css/all.min.css") }}"/>

		<!-- Page CSS -->
		{% block PageCSS %}{% endblock %}

		<style>

			.swal2-container {
				z-index: 99999;
			}

			.ck-editor__editable {
				min-height: 500px;
			}
		</style>

		<!-- Helpers -->
		<script src="{{ includeResource("vendor/js/helpers.js") }}"></script>

		<script src="{{ includeResource("js/config.js") }}"></script>
	</head>

	<body>
		{% block LayoutWrapper %}
			<!-- Layout wrapper -->
			<div class="layout-wrapper layout-content-navbar">
				<div class="layout-container">

					{{ include("Components/Navbar/Base.twig") }}

					<!-- Layout container -->
					<div
						class="layout-page">
						<!-- Navbar -->
						{% include "Components/Navbar/Topbar.twig" %}

						<!-- Content wrapper -->
						<div class="content-wrapper">

							<div class="container-xxl flex-grow-1 container-p-y">


								<h1>{{ pageTitle }}</h1>
								<hr>
								{% block Content %}{% endblock %}

							</div>

							{{ include("Footer.twig") }}

							<div class="content-backdrop fade"></div>
						</div>
						<!-- Content wrapper -->
					</div>
					<!-- / Layout page -->
				</div>

				<!-- Overlay -->
				<div class="layout-overlay layout-menu-toggle"></div>
			</div>
			<!-- / Layout wrapper -->
		{% endblock %}

		<!-- Core JS -->
		<!-- build:js assets/vendor/js/core.js -->
		<script src="{{ includeResource("vendor/libs/jquery/jquery.js") }}"></script>
		<script src="{{ includeResource("vendor/libs/popper/popper.js") }}"></script>
		<script src="{{ includeResource("vendor/js/bootstrap.js") }}"></script>
		<script src="{{ includeResource("vendor/libs/perfect-scrollbar/perfect-scrollbar.js") }}"></script>

		<script src="{{ includeResource("vendor/js/menu.js") }}"></script>
		<!-- endbuild -->

		<!-- Vendors JS -->
		<script src="{{ includeResource("vendor/libs/apex-charts/apexcharts.js") }}"></script>

		<!-- Main JS -->
		<script src="{{ includeResource("js/main.js") }}"></script>
		<script src="{{ includeResource("js/ui-popover.js") }}"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>

		<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

		<script>
			let csrf = "{{ getCSRF() }}";
let translation = {{ AllTranslations|json_encode|raw }};
		</script>


		{% if load_quill ?? false %}

			<script type="importmap">
				{
							"imports": {
								"ckeditor5": "{{ includeResource("vendor/libs/ckeditor5/ckeditor5.js") }}",
								"ckeditor5/": "{{ includeResource("vendor/libs/ckeditor5/") }}"
							}
						}
			</script>
			<!--<script src="https://cdn.ckeditor.com/ckeditor5/39.0.1/classic/ckeditor.js"></script>-->
			<script src="https://cdnjs.cloudflare.com/ajax/libs/elfinder/2.1.65/js/elfinder.min.js" integrity="sha512-WJC7xcYftu+jl2taNq8hF9tdT8BuizNJSazzKJSVGaHselut9uQhe0ag0R3Hj6j2Cg7TGN9D0s+iH05XtcR/MA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

			<script type="module" src="{{ includeResource("js/CMSControl/editor.js") }}"></script>
		{% endif %}

		<!-- Page JS -->
		{% block PageJs %}{% endblock %}
	</body>
</html></body></html>
