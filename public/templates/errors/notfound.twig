{% endblock %}
{% block scripts %}
	<script>

		$(document).ready(function () {
var map = L.map('map').setView([
50.874298, 6.167580
], 16);
L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
maxZoom: 19,
attribution: '&copy; <a href="http://www.openstreetmap.org/copyright">OpenStreetMap</a>'
}).addTo(map);

var marker = L.marker([50.874298, 6.167580]).addTo(map);

marker.bindPopup("Magen-Darm-Praxis Alsdorf<br><br>Hubertusstraße 23 • 52477 Alsdorf<br>Telefon: 02404-93933").openPopup();
});
	</script>
{% endblock %}
