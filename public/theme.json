{
  "name": "Crisp",
  "author": "Justin Ren√© Back <jb@pixelcowboys.de>",
  "description": "Default theme for CrispCMS",
  "type": 1,
  "hookFile": "ThemeHook.php",
  "strict_autoloading": false,
  "autoload":[
    "includes/class",
    "includes/vendor",
    "/data/plugins"
  ],
  "onBoot": [
    "includes/boot/generate_code.php"
  ],
  "onInstall": {
    "createTranslationKeys": "translations/",
    "createKVStorageItems": {
      "site_logo": "logo.png",
      "site_name": "My Website",
      "page_delimiter": "--"
    },
    "activateDependencies": []
  },
  "onUninstall": {
    "deleteData": true,
    "deactivateDependencies": [],
    "purgeDependencies": []
  }
}
